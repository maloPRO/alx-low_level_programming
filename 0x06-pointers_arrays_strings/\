#include "main.h"

/**
 * _strcat-concatenates strings
 * @dest: destination
 * @src: source
 * Return: string
 */
char *_strcat(char *dest, char *src)
{
	int len1 = 0;
	int len2 = 0;
	int i;
	int j;

	while (dest[len1] != '\0')
	{
		len1++;
	}
	while (src[len2] != '\0')
	{
		len2++;
	}
	for (i = 0; i < len1; i++)
	{
		if (dest[i] != '\0')
		{
			_putchar(dest[i];
		}
	}
	for (j = 0; j < len2; j++)
	{
		_putchar(src[j];
	}
	_putchar('\n');
}
